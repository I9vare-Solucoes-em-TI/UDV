  object sqlBoletos: TI9Query
    AfterScroll = sqlBoletosAfterScroll
    OnCalcFields = sqlBoletosCalcFields
    Connection = dtmControles.DB
    FetchOptions.AssignedValues = [evMode]
    FetchOptions.Mode = fmAll
    SQL.Strings = (
      
        'SELECT B.BOLETA_ID, B.VALOR, B.PESSOA_ID, B.PESSOA_GRUPO_ID, B.D' +
        'ATA_EMISSAO,'#13#10'       P.NOME, G.DESCRICAO, B.CAMINHO_ARQUIVO, TRA' +
        'NSMITIDO'#13#10'FROM T_BOLETA B'#13#10'  LEFT OUTER JOIN T_PESSOA P'#13#10'  ON B.' +
        'PESSOA_ID = P.PESSOA_ID'#13#10'  LEFT OUTER JOIN T_PESSOA_GRUPO G'#13#10'  O' +
        'N B.PESSOA_GRUPO_ID = G.PESSOA_GRUPO_ID'#13#10'WHERE B.SITUACAO = '#39'1'#39#13 +
        #10'    AND B.ENVIO_EMAIL = :EMAIL'#13#10'     AND B.CEDENTE_ID = :CEDENT' +
        'E_ID'#13#10'     AND B.ANO_MES_BOLETO = :ANO_MES_BOLETO'#13#10'ORDER BY P.NO' +
        'ME, G.DESCRICAO')
    Left = 208
    Top = 96
    ParamData = <
      item
        Name = 'EMAIL'
        DataType = ftString
        ParamType = ptInput
      end
      item
        Name = 'CEDENTE_ID'
        DataType = ftString
        ParamType = ptInput
      end
      item
        Name = 'ANO_MES_BOLETO'
        DataType = ftString
        ParamType = ptInput
      end>
    object sqlBoletosselecionar: TBooleanField
      FieldKind = fkInternalCalc
      FieldName = 'selecionar'
    end
    object sqlBoletosBOLETA_ID: TBCDField
      FieldName = 'BOLETA_ID'
      Precision = 20
      Size = 2
    end
    object sqlBoletosVALOR: TBCDField
      FieldName = 'VALOR'
      Precision = 20
      Size = 3
    end
    object sqlBoletosPESSOA_ID: TBCDField
      FieldName = 'PESSOA_ID'
      Precision = 20
      Size = 3
    end
    object sqlBoletosPESSOA_GRUPO_ID: TBCDField
      FieldName = 'PESSOA_GRUPO_ID'
      Precision = 20
      Size = 2
    end
    object sqlBoletoscalc_Nome: TStringField
      FieldKind = fkCalculated
      FieldName = 'calc_Nome'
      Size = 120
      Calculated = True
    end
    object sqlBoletosDATA_EMISSAO: TSQLTimeStampField
      FieldName = 'DATA_EMISSAO'
    end
    object sqlBoletosNOME: TStringField
      FieldName = 'NOME'
      Size = 120
    end
    object sqlBoletosDESCRICAO: TStringField
      FieldName = 'DESCRICAO'
      Size = 60
    end
    object sqlBoletosCAMINHO_ARQUIVO: TStringField
      FieldName = 'CAMINHO_ARQUIVO'
      Size = 260
    end
    object sqlBoletosTRANSMITIDO: TStringField
      FieldName = 'TRANSMITIDO'
      Size = 1
    end
  end
