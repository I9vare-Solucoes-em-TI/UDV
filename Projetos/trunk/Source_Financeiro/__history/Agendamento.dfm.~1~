  object sqlLivroAgendamento: TSimpleDataSet
    Aggregates = <>
    Connection = dtmControles.DB
    DataSet.CommandText = 
      'SELECT LA.*'#13#10'FROM J_LIVRO_AGENDAMENTO LA'#13#10'  LEFT OUTER JOIN J_PE' +
      'SSOA P ON'#13#10'  LA.PESSOA_ID = P.PESSOA_ID'#13#10'WHERE LA.SITUACAO = '#39'1'#39 +
      #13#10'  AND LA.OPERACAO = '#39'D'#39#13#10'ORDER BY LA.HISTORICO'
    DataSet.MaxBlobSize = -1
    DataSet.Params = <>
    Params = <>
    AfterScroll = sqlLivroAgendamentoAfterScroll
    Left = 348
    Top = 326
