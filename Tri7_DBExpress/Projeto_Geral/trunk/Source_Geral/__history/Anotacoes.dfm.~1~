  object sqlAnotacao: TSimpleDataSet
    Aggregates = <>
    DataSet.CommandText = 
      'SELECT * FROM R_ANOTACAO'#13#10'WHERE USUARIO_ID = :USUARIO_ID'#13#10'ORDER ' +
      'BY TITULO'
    DataSet.MaxBlobSize = -1
    DataSet.Params = <
      item
        DataType = ftFMTBcd
        Name = 'USUARIO_ID'
        ParamType = ptInput
      end>
    Params = <>
    Left = 305
    Top = 280
    object sqlAnotacaoANOTACAO_ID: TFMTBCDField
      FieldName = 'ANOTACAO_ID'
      Required = True
      Precision = 15
      Size = 2
    end
    object sqlAnotacaoUSUARIO_ID: TFMTBCDField
      FieldName = 'USUARIO_ID'
      Precision = 15
      Size = 2
    end
    object sqlAnotacaoDATA: TSQLTimeStampField
      FieldName = 'DATA'
    end
    object sqlAnotacaoTITULO: TStringField
      FieldName = 'TITULO'
      Size = 120
    end
  end
  object dtsAnotacao: TDataSource
