  object sqlAssociadosAtivos: TSimpleDataSet
    Aggregates = <>
    DataSet.CommandText = 
      'SELECT NOME, PESSOA_ID, GRAU, SITUACAO, CEDENTE_ID'#13#10'FROM T_PESSO' +
      'A'#13#10'WHERE SITUACAO IN ('#39'1'#39', '#39'3'#39')'#13#10'     AND CEDENTE_ID = :CEDENTE_' +
      'ID'#13#10'ORDER BY NOME'
    DataSet.MaxBlobSize = -1
    DataSet.Params = <
      item
        DataType = ftBCD
        Name = 'CEDENTE_ID'
        ParamType = ptInput
      end>
    Params = <>
    Left = 304
    Top = 332
