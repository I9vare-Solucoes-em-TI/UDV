  object sqlVerificar: TSimpleDataSet
    Aggregates = <>
    DataSet.CommandText = 
      'SELECT *'#13#10'FROM T_PESSOA'#13#10'WHERE CEDENTE_ID = :CEDENTE_ID'#13#10'ORDER B' +
      'Y NOME'
    DataSet.MaxBlobSize = -1
    DataSet.Params = <
      item
        DataType = ftFMTBcd
        Name = 'CEDENTE_ID'
        ParamType = ptInput
      end>
    Params = <>
    Left = 308
    Top = 164
    object sqlVerificarNOME: TStringField
      FieldName = 'NOME'
      Size = 120
    end
