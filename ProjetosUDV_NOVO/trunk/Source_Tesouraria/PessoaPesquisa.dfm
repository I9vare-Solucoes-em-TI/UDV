  object sqlPesquisa: TSimpleDataSet
    Aggregates = <>
    Connection = dtmControles.DB
    DataSet.CommandText = 
      'SELECT P1.PESSOA_ID, P1.NOME, P1.CPF, P1.RG, P1.SELECIONADO,'#13#10'P1' +
      '.TELEFONES, P1.SITUACAO, P1.GRAU, P1.PESSOA_GRUPO_ID,'#13#10'P1.EMAIL,' +
      ' P1.OBSERVACAO, P1.BOLETO_EMAIL, P1.PESSOA_TERCEIRO_ID,'#13#10'P1.FATU' +
      'RA_TIPO,'#13#10#13#10' CASE'#13#10'   WHEN (P1.PESSOA_TERCEIRO_ID > 0) THEN'#13#10'   ' +
      '(SELECT P2.NOME'#13#10'   FROM T_PESSOA P2'#13#10'   WHERE P2.PESSOA_ID = P1' +
      '.PESSOA_TERCEIRO_ID)'#13#10'  ELSE'#13#10'   (SELECT P2.NOME'#13#10'   FROM T_PESS' +
      'OA P2'#13#10'   WHERE P2.PESSOA_GRUPO_ID = P1.PESSOA_GRUPO_ID'#13#10'    AND' +
      ' P2.EMAIL_GRUPO_PADRAO = '#39'S'#39#13#10'     AND P2.NOME <> P1.NOME)'#13#10'  EN' +
      'D AS PESSOA_ENVIO_EMAIL'#13#10'FROM T_PESSOA P1'
    DataSet.MaxBlobSize = -1
    DataSet.Params = <>
    Params = <>
    AfterScroll = sqlPesquisaAfterScroll
